{
  "name": "buyanov/noextlinks",
  "description": "This plugin hides external links",
  "license": "MIT",
  "require"    : {
    "php": ">=7.1|8.*",
    "ext-json": "*"
  },
  "require-dev": {
    "consolidation/robo": "^4.0.6",
    "phpunit/phpunit": "^10",
    "joomla/utilities": "@dev",
    "joomla/uri": "@dev",
    "joomla/registry": "@dev",
    "joomla/input": "@dev"
  },
  "config": {
    "bin-dir": "scripts"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "Tests/",
      "Buyanov\\NoExtLinks\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "minimum-stability": "dev"
}
