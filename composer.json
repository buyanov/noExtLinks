{
  "name": "buyanov/noextlinks",
  "description": "This plugin hides external links",
  "license": "MIT",
  "require"    : {
    "php": ">=7.1",
    "ext-json": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "consolidation/robo": "^1.0.0",
    "phpunit/phpunit": "^9",
    "joomla/utilities": "^1.6",
    "joomla/uri": "^1.2",
    "joomla/registry": "~1.0",
    "joomla/input": "^1.4"
  },
  "config": {
    "bin-dir": "scripts"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "Tests/",
      "Buyanov\\NoExtLinks\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  }
}
